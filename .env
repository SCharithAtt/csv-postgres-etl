# PostgreSQL Database Configuration
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=etl_database
POSTGRES_USER=etl_user
POSTGRES_PASSWORD=your_secure_password

# File Ingestion Configuration
SOURCE_FOLDER_PATH=/data/input
PROCESSED_FOLDER_PATH=/data/processed
UNPROCESSED_FOLDER_PATH=/data/unprocessed

# Airflow Configuration
AIRFLOW_UID=1000
AIRFLOW_GID=0
AIRFLOW_PROJ_DIR=./orchestration-service

# Logging Configuration
LOG_LEVEL=INFO
LOG_DIR=/logs

# Service Communication
RABBITMQ_DEFAULT_USER=etl_rabbit
RABBITMQ_DEFAULT_PASS=your_rabbit_password
RABBITMQ_HOST=rabbitmq